{
    "name": "empresa/departamento",
    "description": "Sistema de departamento",
    "require": {
        "phpmailer/phpmailer": "^6.10",
        "dompdf/dompdf": "^3.0",
        "phpoffice/phpspreadsheet": "^4.4",
        "phpoffice/phpword": "^1.2",
        "select2/select2": "4.1.0-rc.0",
        "apalfrey/select2-bootstrap-5-theme": "^1.3"
    },
    "autoload": {
        "files": [
            "config/config.php",
            "config/configBitacora.php"
        ],
        "psr-4": {
            "App\\Model\\": "model/",
            "App\\Config\\": "config/"
        },
        "classmap": [
            "model/"
        ]
    },
    "config": {
        "platform-check": false
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    }
}