          <elementProp name="USUARIO" elementType="Argument">
            <stringProp name="Argument.name">USUARIO</stringProp>
            <stringProp name="Argument.value">LigiaDuran</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
