        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Verificar que las Sesiones son Diferentes" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">def session1 = vars.get(&quot;SESSION_ID_1&quot;)
def session2 = vars.get(&quot;SESSION_ID_2&quot;)

log.info(&quot;Sesión 1: &quot; + session1)
log.info(&quot;Sesión 2: &quot; + session2)

if (session1 == null || session2 == null) {
    SampleResult.setResponseMessage(&quot;ERROR: No se pudieron extraer los IDs de sesión&quot;)
    SampleResult.setSuccessful(false)
} else if (session1.equals(session2)) {
    SampleResult.setResponseMessage(&quot;¡VULNERABLE! Los IDs de sesión son idénticos: &quot; + session1)
    SampleResult.setSuccessful(false)
} else {
    SampleResult.setResponseMessage(&quot;SEGURO: Los IDs de sesión son únicos\nSesión 1: &quot; + session1 + &quot;\nSesión 2: &quot; + session2)
    SampleResult.setSuccessful(true)
}
</stringProp>
        </JSR223Sampler>
