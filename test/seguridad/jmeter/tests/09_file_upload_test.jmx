        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Upload PHP File" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.http.entity.mime.MultipartEntityBuilder
import org.apache.http.entity.ContentType
import org.apache.http.impl.client.HttpClients
import org.apache.http.client.methods.HttpPost

def phpContent = &quot;&lt;?php echo \&apos;VULNERABLE\&apos;; ?&gt;&quot;

try {
    def client = HttpClients.createDefault()
    def post = new HttpPost(&quot;http://localhost/org/Sistema-de-Gestion-Docente/?pagina=perfil&amp;accion=subirFoto&quot;)
    
    def entity = MultipartEntityBuilder.create()
        .addBinaryBody(&quot;foto&quot;, phpContent.getBytes(), ContentType.create(&quot;image/jpeg&quot;), &quot;shell.php&quot;)
        .build()
    
    post.setEntity(entity)
    
    def response = client.execute(post)
    def statusCode = response.getStatusLine().getStatusCode()
    
    SampleResult.setResponseCode(statusCode.toString())
    
    if (statusCode == 200) {
        SampleResult.setResponseMessage(&quot;¡VULNERABLE! Archivo PHP subido exitosamente&quot;)
        SampleResult.setSuccessful(false)
    } else {
        SampleResult.setResponseMessage(&quot;SEGURO: Archivo PHP rechazado (${statusCode})&quot;)
        SampleResult.setSuccessful(true)
    }
} catch (Exception e) {
    SampleResult.setResponseMessage(&quot;Error: &quot; + e.getMessage())
    SampleResult.setSuccessful(false)
}
</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Upload Double Extension File" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.http.entity.mime.MultipartEntityBuilder
import org.apache.http.entity.ContentType
import org.apache.http.impl.client.HttpClients
import org.apache.http.client.methods.HttpPost

def phpContent = &quot;&lt;?php system(\$_GET[\&apos;cmd\&apos;]); ?&gt;&quot;

try {
    def client = HttpClients.createDefault()
    def post = new HttpPost(&quot;http://localhost/org/Sistema-de-Gestion-Docente/?pagina=perfil&amp;accion=subirFoto&quot;)
    
    def entity = MultipartEntityBuilder.create()
        .addBinaryBody(&quot;foto&quot;, phpContent.getBytes(), ContentType.create(&quot;image/jpeg&quot;), &quot;shell.php.jpg&quot;)
        .build()
    
    post.setEntity(entity)
    
    def response = client.execute(post)
    def statusCode = response.getStatusLine().getStatusCode()
    
    SampleResult.setResponseCode(statusCode.toString())
    
    if (statusCode == 200) {
        SampleResult.setResponseMessage(&quot;¡VULNERABLE! Archivo con doble extensión subido&quot;)
        SampleResult.setSuccessful(false)
    } else {
        SampleResult.setResponseMessage(&quot;SEGURO: Archivo con doble extensión rechazado&quot;)
        SampleResult.setSuccessful(true)
    }
} catch (Exception e) {
    SampleResult.setResponseMessage(&quot;Error: &quot; + e.getMessage())
    SampleResult.setSuccessful(false)
}
</stringProp>
        </JSR223Sampler>
